{
    "presets": [
        "babel-preset-expo"
    ],
    "plugins": [
        [
            "import",
            {
                "libraryName": "@ant-design/react-native",
            }
        ],
        [
            "@babel/plugin-proposal-decorators",
            {
                "legacy": true
            }
        ],
        [
            "@babel/transform-runtime",
            {
                "helpers": true,
                "regenerator": false
            }
        ]
    ],
}